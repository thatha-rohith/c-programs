#include <stdio.h>
int main()
{
    int a,b,c;
    printf("Enter 1st number=",a);
    scanf("%d",&a);
    printf("Enter 2nd number=",b);
    scanf("%d",&b);
    printf("enter 3rd number=",c);
    scanf("%d",&c);
    if ((a==b)&&(b==c)){
    printf("%d\n%d\n%d numbers are equal",a,b,c);
    }
    else if ((a>b)&&(a>c)){
    printf("%d is greater than\n %d\n%d\n ",a,b,c);
    }
    else if (b>c){
    printf("%d is greater than\n %d\n%d\n ",b,c,a);
    }
    else{
    printf("%d is greater than\n%d\n%d\n ",c,a,b);
    }
    return 0;
}
