#include <stdio.h>
void main()
{
    int a,b;
    printf("Enter 1st number=",a);
    scanf("%d",&a);
    printf("Enter 2nd number=",b);
    scanf("%d",&b);
    if (a>b)
    printf("%d is greater than %d ",a,b);
    else if (a<b)
    printf("%d is greater than %d ",b,a);
    else
    printf("both numbers are equal");
    
}
